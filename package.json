{
  "name": "webpack_2_dev_kit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "transpile-script": "babel -d ./sitemap ./sitemap-script -s",
    "sport-script": "babel -d ./sportSitemap ./venuesSitemap -s",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "start-message": "babel-node tools/startMessage.js",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel  open:src lint:watch",
    "prebuild": "npm run clean-dist",
    "build": "babel-node tools/build.js && npm run open:dist",
    "testServer": "babel-node tools/mainServer.js",
    "static-build": "babel-node tools/build.js",
    "clean-server": "rm -rf production && mkdir production",
    "build-files": "babel -d ./production/venueListing ./src -s",
    "copy-css": "cp -R ./src/styles production/venueListing",
    "build-server": "babel -d ./production ./tools/mainServer.js ./tools/index.js ./tools/venueListing.js ./tools/nodeApi.js -s",
    "copy-static": "mkdir production/static && cp ./dist/venueListing.css ./dist/venueListing.js ./dist/vendor.js ./production/static/",
    "copy-manifest": "cp ./manifest/manifest.json  ./production/static/",
    "build-production": "npm run static-build && npm run clean-server  && npm run build-files && npm run build-server && npm run copy-css ",
    "server-render": "node production/tools/mainServer.js"
  },
  "author": "Sourabh garg",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "6.5.4",
    "babel-cli": "6.18.0",
